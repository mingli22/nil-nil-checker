{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd client && npm install && vite build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "functions": {
    "api/matches.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/matches/(.*)",
      "dest": "/api/matches.js"
    }
  ]
}